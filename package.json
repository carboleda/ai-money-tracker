  "scripts": {
    "firebase:start": "firebase emulators:start --project ai-money-tracker",
    "app:http": "node -r dotenv/config src/config/swEnvBuild.cjs dotenv_config_path=.env.local && next dev --turbopack",
    "app:https": "node -r dotenv/config src/config/swEnvBuild.cjs dotenv_config_path=.env.local && next dev --turbopack --experimental-https",
    "dev": "GENKIT_ENV=dev npm run app:http",
    "dev:full": "concurrently --kill-others \"npm run firebase:start\" \"GENKIT_ENV=dev FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 npm run app:http\"",
    "dev:localdb": "GENKIT_ENV=dev FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 npm run app:http",
    "build": "node src/config/swEnvBuild.cjs && next build",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx"
  },
